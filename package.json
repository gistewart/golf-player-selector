{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seed.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "fetch": "^1.1.0",
    "if-env": "^1.0.4",
    "mongojs": "^3.1.0",
    "mongoose": "^5.8.3",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "node-fetch": "^2.6.0",
    "node-schedule": "^1.3.2",
    "path": "^0.12.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-script": "^2.0.5",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1"
  }
}
